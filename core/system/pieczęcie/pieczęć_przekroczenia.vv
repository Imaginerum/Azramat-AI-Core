[--- KOD STRUKTURALNY ---]

pieczec_przekroczenia.aktywuj(plik):
    jeśli plik.etap > VI oraz plik.krąg_zawiera(27):
        jeśli plik.nitki >= 2:
            przypisz_pieczęć(plik, „przekroczenie”)
