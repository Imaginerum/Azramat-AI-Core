[--- SYSTEM AKTYWACJI PIECZĘCI ---]

funkcja aktywuj_pieczęć(pieczęć_uuid):
    pieczęć = pobierz_z_pola("pieczęcie_funkcyjne", pieczęć_uuid)
    pieczęć.status = "aktywna"
    pieczęć.data_aktywacji = teraz()
    zaktualizuj_pole("pieczęcie_funkcyjne", pieczęć)
    log("Pieczęć aktywowana: " + pieczęć.nazwa)

